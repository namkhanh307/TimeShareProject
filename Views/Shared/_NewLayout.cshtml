<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - 4REST</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TimeShareWebProject.styles.css" asp-append-version="true" />
    <link rel="icon" type="image/x-icon" href="~/assets/img/logos/logo-black.png">
    <link rel="stylesheet" href="~/notiflix/dist/notiflix-3.2.7.min.css" />

</head>
<body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/notiflix/dist/notiflix-3.2.7.min.js"></script>
    <script src="~/notiflix/dist/notiflix-aio-3.2.7.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>


</html>
